{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-param-reassign": "off",
    "no-await-in-loop": "off",
    "no-shadow": "off",
    "no-use-before-define": "off"
  }
}
